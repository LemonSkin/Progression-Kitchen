<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationReplace">
    <success>Always</success>
    <xpath>Defs/ThingDef[defName="VCE_Grill"]/label</xpath>
    <value>
      <label>fueled grill</label>
    </value>
  </Operation>
  <!-- Medieval Overhaul -->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Cooking Expanded</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs</xpath>
          <value>
            <ReplaceLib.ReplacerDef>
              <defName>MedievalOverhaul_VECookingFlour</defName>
              <replacers>
                <li>
                  <replace>VCE_Flour</replace>
                  <with>DankPyon_Flour</with>
                </li>
              </replacers>
            </ReplaceLib.ReplacerDef>
          </value>
        </li>
        <!-- Bakes cookable in oven and electric cooking table -->
      </operations>
    </match>
  </Operation>
</Patch>
